classDiagram
    レート o-- 入力値
    レート o-- レート値
    class レート{
        -入力値 id
        -レート値 rate
        +制約チェック()
    }
    class 入力値{
        -int 値
        +制約チェック()
    }
    class レート値{
        -float 値
        +制約チェック()
    }
    class 計算結果値{
        -float 値
        +制約チェック()
    }
